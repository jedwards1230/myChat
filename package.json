{
	"name": "@mychat/monorepo",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"export:docker": "docker build -t mychat:local .",
		"compose": "docker compose -f compose.yml",
		"compose:test": "docker compose -f compose.yml -f compose.test.yml",
		"lint": "turbo lint",
		"tsc": "turbo tsc",
		"export": "turbo export",
		"build": "turbo build --cache-dir=.turbo",
		"dev": "TURBO_EXPERIMENTAL_UI=true turbo dev",
		"start": "turbo start",
		"start:prod": "turbo start:prod",
		"compose:prod": "docker compose -f production.yml",
		"clean": "turbo clean && rm -rf node_modules && yarn cache clean"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"resolutions": {
		"eslint": "9.1.1",
		"markdown-it": "14.0.0",
		"react-native-svg": "15.2.0"
	},
	"devDependencies": {
		"@mychat/tsconfig": "workspace:*",
		"turbo": "^1.13.3"
	},
	"packageManager": "yarn@4.1.1+sha512.ec40d0639bb307441b945d9467139cbb88d14394baac760b52eca038b330d16542d66fef61574271534ace5a200518dabf3b53a85f1f9e4bfa37141b538a9590"
}
